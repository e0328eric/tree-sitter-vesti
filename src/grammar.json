{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "vesti",
  "rules": {
    "vesti_content": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "docclass_decl"
        },
        {
          "type": "SYMBOL",
          "name": "singlepkg_decl"
        },
        {
          "type": "SYMBOL",
          "name": "multipkg_decl"
        },
        {
          "type": "SYMBOL",
          "name": "env_decl"
        }
      ]
    },
    "docclass_decl": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "KEYWORD_docclass"
        },
        {
          "type": "SYMBOL",
          "name": "class_pkg_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "options"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "singlepkg_decl": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "KEYWORD_importpkg"
        },
        {
          "type": "SYMBOL",
          "name": "class_pkg_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "options"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "multipkg_decl": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "KEYWORD_importpkg"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "class_pkg_name"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "options"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\n"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "class_pkg_name"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "options"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\n"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "env_decl": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "KEYWORD_useenv"
        },
        {
          "type": "SYMBOL",
          "name": "env_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "env_arg"
          }
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "PATTERN",
            "value": "[^}]"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "options": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "PATTERN",
            "value": "[^,]"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "REPEAT1",
                    "content": {
                      "type": "PATTERN",
                      "value": "[^,]"
                    }
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "env_arg": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "mandantory_arg"
        },
        {
          "type": "SYMBOL",
          "name": "optional_arg"
        }
      ]
    },
    "mandantory_arg": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^)]"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "optional_arg": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "[^\\]]"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "class_pkg_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z0-9-]+"
      }
    },
    "env_name": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z][A-Za-z0-9-]*(\\*)*"
      }
    },
    "KEYWORD_docclass": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "docclass"
      }
    },
    "KEYWORD_importpkg": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "importpkg"
      }
    },
    "KEYWORD_useenv": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "useenv"
      }
    },
    "digit": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]"
      }
    },
    "ascii_letter": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[A-Za-z]"
      }
    },
    "letter": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\p{L}",
        "flags": "u"
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "%([^\\*\\!\\-])[^\\n]*"
      }
    },
    "multiline_comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "%\\*[\\s\\S]*?\\*\\%"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "multiline_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}